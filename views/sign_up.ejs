        <div class="col-sm-6">
          <input class="form-control" id="inputPassword" name="pass" type="password" placeholder="Password">
        <p class="help-block">number, 8 or more characters including special characters</p>
        </div>
        </div>
          <div class="form-group">
              <label class="col-sm-3 control-label" for="inputPasswordCheck">Confirm Password</label>
             <div class="col-sm-6">
              <input class="form-control" id="inputPasswordCheck" name="c_pass" type="password" placeholder="Confirm Password">
                <p class="help-block">Please check your password once more</p>
             </div>
          </div>

        <div class="form-group">
            <label class="col-sm-3 control-label" for="inputName">Name</label>
          <div class="col-sm-6">
            <input class="form-control" id="inputName" name="w_name" type="text" placeholder="name">
          </div>
        </div>
        <div class="form-group">
          <label class="col-sm-3 control-label" for="inputName">Date of birth</label>
        <div class="col-sm-6">
          <input class="form-control" id="inputName" name="w_birth" type="text" placeholder="Date of birth">
        </div>
      </div>
      <div class="form-group">
        <label class="col-sm-3 control-label" for="inputName">Address</label>
      <div class="col-sm-6">
        <input class="form-control" id="inputName" name="w_addr" type="text" placeholder="Address">
      </div>
    </div>
    <div class="form-group" id="divAccountNumber">
      <label class="col-sm-3 control-label" for="inputNumber">Phone number</label>
      <div class="col-sm-6">
        <input
          type="tel"
          class="form-control onlyNumber"
          id="AccountNumber"
          name="w_phone"
          data-rule-required="true"
          placeholder="Phone number"
          maxlength="11"
        />
      </div>
            <div class="col-sm-6" data-toggle="buttons">
              <label class="btn btn-warning active">
                <input id="agree" type="checkbox" autocomplete="off" chacked>
                  <span class="fa fa-check"></span>
              </label>
              <a href="#">Terms of Use</a> agree
            </div>
          </div>
        <div class="form-group">
          <div class="col-sm-12 text-center">
            <button class="btn btn-primary" name = "signup">sign up</button>
            <button class="btn btn-danger" name = "cancel">Cancel sign up</button>
          </div>
        </div>
        </form>
          <hr>
